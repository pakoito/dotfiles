# ======================
# Brewbot inbox.
# ======================
set from      = "esteban@brewbot.io"
set spoolfile = "+brewbot/Inbox"
set sendmail  = "/usr/local/bin/msmtp -C $HOME/.mutt/msmtprc -a brewbot"
set signature = $HOME/.mutt/accounts/brewbot.signature

# Other special folders.
set mbox      = "+brewbot/Inbox"
set postponed = "+brewbot/Drafts"

# Colors
source $HOME/.mutt/colors/mutt-colors-solarized-light-256.muttrc

# vim: ft=muttrc
