# Export Variables
export ZSH=$HOME/.oh-my-zsh
export GOPATH=/usr/local/opt/go
export PATH=$PATH:$GOPATH/bin
export PATH="/usr/local/bin":$PATH
export PATH="/usr/bin/gcov":$PATH
export PATH="/usr/local/bin/mergepbx":$PATH
